<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="TOP_CENTER" spacing="10.0" stylesheets="@../css/styles.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="expenseTable" prefHeight="400.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="dateColumn" prefWidth="100.0" text="Date" />
          <TableColumn fx:id="expenseColumn" prefWidth="150.0" text="Description" />
          <TableColumn fx:id="amountColumn" prefWidth="100.0" text="Amount" />
          <TableColumn fx:id="categoryColumn" prefWidth="100.0" text="Category" />
          <TableColumn fx:id="actionsColumn" prefWidth="150.0" text="Actions" />
        </columns>
      </TableView>
      <Button mnemonicParsing="false" onAction="#handleBack" text="Back to Main" styleClass="btn-secondary" />
   </children>
</VBox>
